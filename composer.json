{
  "name": "jeylabs/vsts",
  "type": "library",
  "description": "Visual Studio Team Services (VSTS) and Team Foundation Server (TFS) REST API PHP Client",
  "keywords": [
    "microsoft",
    "visual studio team services rest client",
    "team foundation server rest client",
    "vsts",
    "tfs"
  ],
  "homepage": "https://github.com/jeylabs/vsts",
  "license": "MIT",
  "authors": [
    {
      "name": "S A Sureshkumar",
      "email": "suresh.ceymplon@jeylabs.com.au",
      "homepage": "https://jeylabs.com"
    }
  ],
  "require": {
    "php": "~7.0",
    "guzzlehttp/guzzle": "~6.2",
    "illuminate/support": "5.3.*|5.4.*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*",
    "scrutinizer/ocular": "~1.1",
    "squizlabs/php_codesniffer": "~2.3"
  },
  "autoload": {
    "psr-4": {
      "Jeylabs\\VSTS\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jeylabs\\VSTS\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
